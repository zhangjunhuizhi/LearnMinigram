<!--components/w-tab-control/w-tab-control.wxml-->
<view class='container'>
  <block wx:for="{{titles}}" wx:key="{{index}}">
    <view class='tab-item {{currentIndex == index ? "active" :""}}' bind:tap="handleItemClick" data-index= "{{index}}">
    <text>{{item}}</text>
    </view>
  </block>
</view>